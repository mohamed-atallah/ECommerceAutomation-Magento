<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression" parallel="tests" thread-count="16">

    <test verbose="10" preserve-order="true" name="Landing Page Test Suite 1">
        <parameter name="Browser" value="chrome"/>
        <classes>
            <class name="com.magento.TestCases.LandingTest"/>
        </classes>
    </test>

    <test verbose="10" preserve-order="true" name="Landing Page Test Suite 2">
        <parameter name="Browser" value="firefox"/>
        <classes>
            <class name="com.magento.TestCases.LandingTest"/>
        </classes>
    </test>

    <test verbose="10" preserve-order="true" name="Landing Page Test Suite 3">
        <parameter name="Browser" value="edge"/>
        <classes>
            <class name="com.magento.TestCases.LandingTest"/>
        </classes>
    </test>

    <test verbose="10" preserve-order="true" name="Landing Page Test Suite 4">
        <parameter name="Browser" value="chromeheadless"/>
        <classes>
            <class name="com.magento.TestCases.LandingTest"/>
        </classes>
    </test>

</suite>
